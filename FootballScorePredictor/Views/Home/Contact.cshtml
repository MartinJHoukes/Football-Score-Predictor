
<div class="mt-3">
    <table id="testTable" class="table table-bordered">
        <thead>
            <tr>
                <td>
                    MatchID
                </td>
                <td>
                    Match Date
                </td>
            </tr>
        </thead>
        <tbody id="divResults">
        </tbody>
    </table>
</div>

<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<link href="~/Content/DataTables/css/jquery.dataTables.min.css" rel="stylesheet" />



<script>

    // Get All Results
    $(document).ready(function () {


        // Load ALl Results
        $.post("/Home/GetAllResults", null).done(function (results) {
            $.each(results, function (index, result) {
                let rRow = "<tr><td>" + result.MatchID + "</td><td>" + result.MatchDate + "</td></tr>"
                $('#divResults').append(rRow);
            });


            $('#testTable').DataTable();
        });

    });

</script>

